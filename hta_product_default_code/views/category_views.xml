<odoo>
  <data>
    <record id="view_product_category_inherit_default" model="ir.ui.view">
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view" />
        <field name="arch" type="xml">
                <xpath expr="//sheet/group[@name='first']/field[@name='parent_id']" position="after">
                    <field name="is_virtual_product" string="Product Support"/>
                    <field name="attribute_ids" string="Caracteristique" class="oe_inline" attrs="{'invisible':[('is_virtual_product','=',False)]}">
                        <tree editable="bottom">
                            <field name="name"/>
                                <field name="value_ids" widget='many2many'>
                                    <tree editable="bottom">
                                        <field name="name"/>
                                        <!--<field name="code"/>-->
                                    </tree>    
                                </field>
                        </tree>
                    </field>
                       
                 </xpath>
        </field>
    </record>
  </data>
</odoo>