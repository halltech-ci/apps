<odoo>
  <data>
    <record id="paybook_report_wizard" model="ir.ui.view" >
        <field name="name">PayBook Report</field>
        <field name="model">paybook.report.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="PayBook Report">
                <group col="4">
                    <field name="date_start"/>
                    <field name="date_end"/>
                </group>
                <group col="4">
                    <field name="salary_structure"/>
                    <field name="employee" invisible='1'/>
                </group>
                
                <footer>
                    <button name="get_report" string="Get Report" type="object" class="oe_highlight"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
      
    <act_window 
        id="action_paybook_report_wizard"
        name="Livre de Paye"
        res_model="paybook.report.wizard"
        view_mode="form"
        target="new"
    />
    <menuitem 
        id="menu_paybook_report_wizard"
        action="action_paybook_report_wizard"
        name = "Livre de Paie "
        sequence="13"
        parent="account.account_reports_management_menu"
        groups='hta_custom_hr.group_hr_paybook_report'
        />
  </data>
</odoo>