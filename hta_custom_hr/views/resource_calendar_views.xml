<odoo>
  <data>
    <record id="hta_view_resource_calendar_tree" model="ir.ui.view">
        <field name="name">hta_resource_calendar_inherit</field>
        <field name="model">resource.calendar.attendance</field>
        <field name="inherit_id" ref="resource.view_resource_calendar_attendance_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree/field[@name='hour_to']" position="after">
                <field name="work_entry_type_id"/>
            </xpath>
        </field>
    </record>
  </data>
</odoo>