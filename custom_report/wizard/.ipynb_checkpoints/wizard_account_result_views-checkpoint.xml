<odoo>
    <record id="account_report_wizard" model="ir.ui.view" >
        <field name="name">Account Result Report</field>
        <field name="model">account.result.report.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Account Result Report">
                <group>
                    <group>
                        <field name="project"/>
                    </group>
                    <group>
                        <field name="date_start"/>
                    </group>
                    <group>
                        <field name="date_end"/>
                    </group>
                </group>
                <footer>
                    <button name="get_report" string="Get Report Pdf" type="object" class="oe_highlight"/>
                    <button name="get_generate_xlsx_report" string="Generate Excel" type="object" class="oe_highlight"/>
    
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <act_window 
        id="action_account_result_report_wizard"
        name="Account Result Report"
        res_model="account.result.report.wizard"
        view_mode="form"
        target="new"
        />

    <menuitem 
        id="menu_account_result_report_wizard"
        action="action_account_result_report_wizard"
        name = "Rapport Account Result"
        sequence="12"
        parent="account.account_reports_management_menu"
        />
</odoo>