<odoo>
  <data>
    <record model="ir.ui.view" id="account_cash_view_inherit">
        <field name="name">account.cash.inherit.view</field>
        <field name="model">account.bank.statement</field>
        <field name="inherit_id" ref="account.view_bank_statement_form"/>
        <field name="arch" type="xml">
            <!-- Insert account object-->
          <xpath expr="//sheet/notebook/page[@name='statement_line_ids']/field[@name='line_ids']/tree/field[@name='name']" position="after">
              
            <field name="project_id"  attrs="{'readonly' : [('journal_entry_ids', '!=', [])] }"/>
              
          </xpath>

        </field>
    </record>
  </data>
</odoo>