<odoo>
  <data>
    <record model="ir.ui.view" id="sale_order_line_view_inherit">
        <field name="name">sale.order.inherit.view</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
        <!-- Insert secondary unit in report-->
          <xpath expr="//sheet/notebook/page[@name='order_lines']/field[@name='order_line']/form/group/group/field[@name='price_total']" position="after">
            <field name='sde_price_unite' string="Seconde Price Unite"/>
          </xpath>
         
          <xpath expr="//sheet/notebook/page[@name='order_lines']/field[@name='order_line']/tree/field[@name='price_unit']" position="after">
            <field name='sde_price_unite' string="Seconde Price Unite"/>
          </xpath>
           
        </field>
    </record>
  </data>
</odoo>