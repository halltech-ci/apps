<odoo>
  <data>
    <record id="hta_product_attribute_form_view" model="ir.ui.view">
        <field name="name">hta_product_attribute</field>
        <field name="model">product.attribute</field>
        <field name="inherit_id" ref="product_variant_default_code.view_product_attribute_form"/>
        <field name="arch" type="xml">
          <xpath expr="//field[@name='value_ids']" position="before">
              <group>
                  <group>
                      <field name="code_type"/>
                      <field name="code_compute_parameter"/>
                  </group>
                  <group>
                      <field name="code_length"/>
                      <field name="is_automatic_code" widget="boolean_button"/>
                  </group>
              </group>
          </xpath>
          <xpath expr="//field[@name='value_ids']//field[@name='code']" position="after">
            <!--<field name="code" />-->
            <field name="is_manual" widget="boolean_button"/>
          </xpath>
        </field>
      </record>
  </data>
</odoo>